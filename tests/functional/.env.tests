POSTGRES_DB=auth-test
POSTGRES_USER=user
POSTGRES_PASSWORD=password

# auth-app container
APP_CONFIG__DB__URL=postgresql+asyncpg://user:password@test_auth_postgres:5432/auth-test
APP_CONFIG__DB__SYNC_URL=postgresql+psycopg://user:password@test_auth_postgres:5432/auth-test
APP_CONFIG__REDIS__URL=test_auth_redis
APP_CONFIG__REDIS_PORT=6379
APP_CONFIG__DB__ECHO=0
APP_CONFIG__AUTHJWT_SECRET_KEY=secret


# test container
TESTS_CONFIG__PG_URL=postgresql+asyncpg://user:password@test_auth_postgres:5432/auth-test
TESTS_CONFIG__PG_URL_SYNC=postgresql+psycopg://user:password@test_auth_postgres:5432/auth-test
TESTS_CONFIG__REDIS_HOST=test_auth_redis
TESTS_CONFIG__SERVICE_HOST=test_auth_app
TESTS_CONFIG__AUTHJWT_SECRET_KEY=secret
